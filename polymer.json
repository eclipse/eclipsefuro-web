{
  "entrypoint": "index.html",
  "shell": "_page/furo-shell.js",
  "fragments": [
    "_page/api/view-api.js",
    "_page/guide/view-guide.js"
  ],
  "sources": [
    "_assets/**/*",
    "_page/markdown/**/*",
    "_page/images/**/*",
    "_page/assets/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/*.js",
    "node_modules/@polymer/iron-component-page/iron-component-page.js",
    "node_modules/@furo/**/analysis.json",
    "node_modules/@furo/**/CHANGELOG.md"
  ],
  "builds": [
    {
      "preset": "es6-bundled"
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}
